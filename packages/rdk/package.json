{
  "name": "@omnidotdev/rdk",
  "description": "AR.js with react-three-fiber",
  "version": "0.8.0",
  "license": "MIT",
  "type": "module",
  "main": "build/index.js",
  "module": "build/index.js",
  "types": "build/index.d.ts",
  "homepage": "https://github.com/omnidotdev/rdk#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omnidotdev/rdk.git"
  },
  "bugs": {
    "url": "https://github.com/omnidotdev/rdk/issues"
  },
  "files": [
    "build/"
  ],
  "keywords": [
    "react",
    "react-three-fiber",
    "ar.js",
    "ar",
    "three",
    "typescript"
  ],
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js",
      "require": "./build/index.cjs"
    },
    "./fiducial": {
      "types": "./build/fiducial.d.ts",
      "import": "./build/fiducial.js",
      "require": "./build/fiducial.cjs"
    },
    "./geolocation": {
      "types": "./build/geolocation.d.ts",
      "import": "./build/geolocation.js",
      "require": "./build/geolocation.cjs"
    },
    "./immersive": {
      "types": "./build/immersive.d.ts",
      "import": "./build/immersive.js",
      "require": "./build/immersive.cjs"
    }
  },
  "scripts": {
    "build": "vite build",
    "clean": "rm -rf build",
    "dev": "vite build --watch --mode development",
    "format": "biome format --write .",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "test": "vitest run",
    "release": "bun run build && changeset publish"
  },
  "peerDependencies": {
    "@ar-js-org/ar.js": "^3.4.7",
    "@react-three/fiber": "^9.5.0",
    "@react-three/xr": "^6.6.29",
    "locar": "^0.1.8",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "three": "^0.182.0"
  },
  "peerDependenciesMeta": {
    "@ar-js-org/ar.js": {
      "optional": true
    },
    "@react-three/xr": {
      "optional": true
    },
    "locar": {
      "optional": true
    }
  },
  "devDependencies": {
    "@ar-js-org/ar.js": "^3.4.7",
    "@biomejs/biome": "^2.3.11",
    "@react-three/fiber": "^9.5.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/three": "^0.182.0",
    "jsdom": "^27.4.0",
    "locar": "^0.1.8",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "three": "^0.182.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "zustand": "^5.0.9"
  }
}
